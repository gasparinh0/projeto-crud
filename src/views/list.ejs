<!DOCTYPE html>
<html lang="pt-br">
<head>
<%- include('partials/head.ejs') %>
</head>
<body>
    <h1>Hello world!</h1>
    <%- include('partials/navbar.ejs') %>
    
    <ul>
    <% users.forEach((user) => { %>
        <li>
          Nome: <%= user.name %> - Idade: <%= user.age %> - Email: <%= user.email %>
          <a href="/edit?id=<%= user._id %>">[Editar]</a> - 
          <a href="/remove/<%= user._id %>">[Remover]</a>
        </li>
    <% }) %>
    </ul>

    <%- include('partials/foot.ejs') %>
</body>
</html>